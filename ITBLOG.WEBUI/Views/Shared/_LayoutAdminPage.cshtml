<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title | ITBlog</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/AdminAssets/img/favicon.ico")">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/animate.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/educate-custon-icon.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/metisMenu/metisMenu.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/metisMenu/metisMenu-vertical.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/style.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/responsive.css")">
    <link rel="stylesheet" href="@Url.Content("~/AdminAssets/css/popup.css")">

    <script src="@Url.Content("~/AdminAssets/js/vendor/modernizr-2.8.3.min.js")"></script>
    
</head>

<body>

    <div class="left-sidebar-pro">
        <nav id="sidebar" class="">
            <div class="sidebar-header">
                <a href="@Url.Action("Index", "Admin")"><img class="main-logo" src="~/AdminAssets/img/logo/logo.png" alt="" /></a>
                <strong><a href="index.html"><img src="~/AdminAssets/img/logo/logosn.png" alt="" /></a></strong>
            </div>
            <div class="left-custom-menu-adp-wrap comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro">
                    <ul class="metismenu" id="menu1">
                        <li class="active">
                            <a href="@Url.Action("Index", "Admin")">
                                <span class="educate-icon educate-home icon-wrap"></span>
                                <span class="mini-click-non">Home</span>
                            </a>
                        </li>
                        <li>
                            <a title="Landing Page" href="@Url.Action("Logout", "Admin")" aria-expanded="false"><span class="educate-icon educate-event icon-wrap sub-icon-mg" aria-hidden="true"></span> <span class="mini-click-non">Event</span></a>
                        </li>

                        <li id="removable">
                            <a class="has-arrow" href="#" aria-expanded="false"><span class="educate-icon educate-pages icon-wrap"></span> <span class="mini-click-non">Pages</span></a>
                            <ul class="submenu-angle page-mini-nb-dp" aria-expanded="false">
                                <li><a title="Logout" href="@Url.Action("Logout", "Admin")"><span class="mini-sub-pro">Logout</span></a></li>
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div>
        </nav>
    </div>
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="~/AdminAssets/img/logo/logo.png" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-advance-area">
            <div class="header-top-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="header-top-wraper">
                                <div class="row">
                                    <div class="col-lg-1 col-md-0 col-sm-1 col-xs-12">
                                        <div class="menu-switcher-pro">
                                            <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                                                <i class="educate-icon educate-nav"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-7 col-sm-6 col-xs-12">
                                        <div class="header-top-menu tabl-d-n">
                                            <ul class="nav navbar-nav mai-top-nav">
                                                <li class="nav-item">
                                                    <a href="@Url.Action("Index", "Admin")" class="nav-link">Home</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a href="@Url.Action("Index", "Admin")" class="nav-link">About</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                        <div class="header-right-info">
                                            <ul class="nav navbar-nav mai-top-nav header-right-menu">
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                                        <img src="~/AdminAssets/img/product/pro4.jpg" alt="" />
                                                        <span class="admin-name">@User.Identity.Name</span>
                                                        <i class="fa fa-angle-down edu-icon edu-down-arrow"></i>
                                                    </a>
                                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated zoomIn">
                                                        <li>
                                                            <a href="@Url.Action("Logout", "Admin")"><span class="edu-icon edu-locked author-log-ic"></span>Log Out</a>
                                                        </li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="breadcome-heading">
                                            
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <ul class="breadcome-menu">
                                            <li>
                                                <a href="@Url.Action("Index", "Admin")">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li>
                                                <span class="bread-blod">@ViewData["Title"]</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
        <div class="footer-copyright-area">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer-copy-right">
                            <p>Copyright © 2018. All rights reserved. Template by <a href="https://colorlib.com/wp/templates/">Colorlib</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <script src="@Url.Content("~/AdminAssets/js/vendor/jquery-1.12.4.min.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/metisMenu/metisMenu.min.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/metisMenu/metisMenu-active.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/counterup/jquery.counterup.min.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/counterup/waypoints.min.js")"></script>
    <script src="@Url.Content("~/AdminAssets/js/counterup/counterup-active.js")"></script>
   
    @RenderSection("PopupJS", required: false)

</body>

</html>